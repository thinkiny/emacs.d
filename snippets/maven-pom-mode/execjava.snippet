# -*- mode: snippet -*-
# name: execjava
# key: exe
# --

<plugin>
  <groupId>org.codehaus.mojo</groupId>
  <artifactId>exec-maven-plugin</artifactId>
  <version>${1:1.6.0}</version>
  <executions>
    <execution>
      <goals>
        <goal>java</goal>
      </goals>
    </execution>
   </executions>
   <configuration>
     <mainClass>${2:\${mainClassName}}</mainClass>
   </configuration>
</plugin>